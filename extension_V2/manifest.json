{ 
    "manifest_version": 3,
    "name": "Guardian V2",
    "description": "An extension to detect and alert users from suicide risk",
    "version": "2.0",
    "permissions": ["activeTab","history"],
    "host_permissions": ["http://127.0.0.1:5000/*"],
      "matches": ["<all_urls>"],
    "action": {
        "default_title": "Classify Webpage",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        },
        "default_popup": "popup.html"
    },

    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
}